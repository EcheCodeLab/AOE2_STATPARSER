        "\\n\n",
        "WINDOW_SEC = 10  # ventana de 60 segundos para c\u00e1lculo de APM\\n\n",
        "\\n\n",
        "# Regex para reconocer \"aldeano\" en ingl\u00e9s o espa\u00f1ol\\n\n",
        "_VILLAGER_RE = re.compile(r\"(villager|aldean|aldeano)\", re.IGNORECASE)\\n\n",
        "\\n\n",
        "\\n\n",
        "# %% ================= FUNCIONES AUXILIARES\\n\n",
        "\\n\n",
        "def is_villager(unit_name: str | None) -> bool:\\n\n",
        "    \"\"\"Retorna True si unit_name coincide con 'villager' o 'aldeano'.\"\"\"\\n\n",
        "    if not unit_name:\\n\n",
        "        return False\\n\n",
        "    return bool(_VILLAGER_RE.search(unit_name))\\n\n",
        "\\n\n",
        "\\n\n",
        "def load_match(replay_path: str):\\n\n",
        "    \"\"\"Carga un archivo .aoe2record y devuelve el objeto Match de mgz.\"\"\"\\n\n",
        "    with open(replay_path, \"rb\") as fh:\\n\n",
        "        return parse_match(fh)\\n\n",
        "\\n\n",
        "\\n\n",
        "# %% ================= C\u00c1LCULO DE M\u00c9TRICAS\\n\n",
        "\\n\n",
        "def villager_counts(match):\\n\n",
        "    \"\"\"\\n\n",
        "    Cuenta cu\u00e1ntos aldeanos produjo cada jugador:\\n\n",
        "    - Filtra por acci\u00f3n DE_QUEUE (que en Age2DE equivale a TRAIN).\\n\n",
        "    - Extrae unit_id y luego el nombre real desde match.units[unit_id].name.\\n\n",
        "    \"\"\"\\n\n",
        "    counts = {p.number: 0 for p in match.players}\\n\n",
        "    for act in match.actions:\\n\n",
        "        if act.type.name != \"DE_QUEUE\":\\n\n",
        "            continue\\n\n",
        "        unit_id = act.payload.get(\"unit_id\")\\n\n",
        "        if not isinstance(unit_id, int):\\n\n",
        "            continue\\n\n",
        "        try:\\n\n",
        "            unit_name = match.units[unit_id].name\\n\n",
        "        except Exception:\\n\n",
        "            unit_name = act.payload.get(\"unit\")  # fallback\\n\n",
        "        if is_villager(unit_name):\\n\n",
        "            pid = act.player.number if act.player else None\\n\n",
        "            if pid is not None:\\n\n",
        "                counts[pid] += 1\\n\n",
        "    return counts\\n\n",
        "\\n\n",
        "\\n\n",
        "def tc_idle_time(match, base_prod_time: float = 25.0, gap_threshold: float = 27.0):\\n\n",
        "    \"\"\"\\n\n",
        "    Calcula el tiempo total que cada jugador dej\u00f3 ocioso su TC:\\n\n",
        "    - Filtra tambi\u00e9n por DE_QUEUE para detectar producci\u00f3n de aldeanos.\\n\n",
        "    - Si el gap entre dos DE_QUEUE de aldeano excede gap_threshold, se suma (gap - base_prod_time).\\n\n",
        "    \"\"\"\\n\n",
        "    idle = {p.number: 0.0 for p in match.players}\\n\n",
        "    last_train = {p.number: None for p in match.players}\\n\n",
        "    for act in match.actions:\\n\n",
        "        if act.type.name != \"DE_QUEUE\":\\n\n",
        "            continue\\n\n",
        "        unit_id = act.payload.get(\"unit_id\")\\n\n",
        "        if not isinstance(unit_id, int):\\n\n",
        "            continue\\n\n",
        "        try:\\n\n",
        "            name = match.units[unit_id].name\\n\n",
        "        except Exception:\\n\n",
        "            name = act.payload.get(\"unit\")\\n\n",
        "        if not is_villager(name):\\n\n",
        "            continue\\n\n",
        "        pid = act.player.number if act.player else None\\n\n",
        "        if pid is None:\\n\n",
        "            continue\\n\n",
        "        t = act.timestamp.total_seconds()\\n\n",
        "        if last_train[pid] is not None:\\n\n",
        "            gap = t - last_train[pid]\\n\n",
        "            if gap > gap_threshold:\\n\n",
        "                idle[pid] += gap - base_prod_time\\n\n",
        "        last_train[pid] = t\\n\n",
        "    return idle\\n\n",
        "\\n\n",
        "\\n\n",
        "def compute_apm_timeseries(match, window_sec: int = WINDOW_SEC):\\n\n",
        "    \"\"\"\\n\n",
        "    Construye un DataFrame de APM (acciones/minuto) por jugador,\\n\n",
        "    usando ventanas de `window_sec` segundos.\\n\n",
        "    \"\"\"\\n\n",
        "    rows = [\\n\n",
        "        (act.timestamp.total_seconds(), act.player.number)\\n\n",
        "        for act in match.actions\\n\n",
        "        if act.player\\n\n",
        "    ]\\n\n",
        "    if not rows:\\n\n",
        "        return pd.DataFrame()\\n\n",
        "    df = pd.DataFrame(rows, columns=[\"t\", \"player\"])\\n\n",
        "    max_t = df[\"t\"].max()\\n\n",
        "    bins = np.arange(0, max_t + window_sec, window_sec)\\n\n",
        "    apm = {}\\n\n",
        "    for pid in df[\"player\"].unique():\\n\n",
        "        counts, _ = np.histogram(df.loc[df[\"player\"] == pid, \"t\"], bins=bins)\\n\n",
        "        apm[pid] = counts * 60 / window_sec\\n\n",
        "    ts = pd.DataFrame(apm, index=bins[:-1])\\n\n",
        "    ts.index.name = \"time_sec\"\\n\n",
        "    return ts\\n\n",
        "\\n\n",
        "\\n\n",
        "def total_units_created(match):\\n\n",
        "    \"\"\"\\n\n",
        "    Cuenta el total de unidades creadas por jugador:\\n\n",
        "    - Filtra por DE_QUEUE (produce cualquier unidad, no s\u00f3lo aldeano).\\n\n",
        "    \"\"\"\\n\n",
        "    counts = {p.number: 0 for p in match.players}\\n\n",
        "    for act in match.actions:\\n\n",
        "        if act.type.name != \"DE_QUEUE\":\\n\n",
        "            continue\\n\n",
        "        pid = act.player.number if act.player else None\\n\n",
        "        if pid is not None:\\n\n",
        "            counts[pid] += 1\\n\n",
        "    return counts\\n\n",
        "\\n\n",
        "\\n\n",
        "def units_created_by_category(match):\\n\n",
        "    \"\"\"\\n\n",
        "    Divide las unidades creadas en 'civil' vs 'militar':\\n\n",
        "    - Civil: si unit_name coincide con patrones de aldeano, barco de pesca o explorador.\\n\n",
        "    - Militar: resto de unidades.\\n\n",
        "    \"\"\"\\n\n",
        "    categories = {p.number: {\"civil\": 0, \"militar\": 0} for p in match.players}\\n\n",
        "    for act in match.actions:\\n\n",
        "        if act.type.name != \"DE_QUEUE\":\\n\n",
        "            continue\\n\n",
        "        pid = act.player.number if act.player else None\\n\n",
        "        if pid is None:\\n\n",
        "            continue\\n\n",
        "        unit_id = act.payload.get(\"unit_id\")\\n\n",
        "        if not isinstance(unit_id, int):\\n\n",
        "            continue\\n\n",
        "        try:\\n\n",
        "            name = match.units[unit_id].name\\n\n",
